package com;

public class MethodOverloading {
	
	void add(int a, int b)
	{
		int sum = a + b;
		System.out.println("The value of sum" + sum);
	}
	void add(int a, int b, int c)
	{
		int sum = a + b + c;
		System.out.println("The value of sum" + sum);
	}

	public static void main(String[] args) {
		MethodOverloading obj1 = new MethodOverloading();
		obj1.add(5, 5);
		obj1.add(2,  6, 2);
		
	}

}

